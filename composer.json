{
    "name": "jadb/groovehq"
    , "description": "GrooveHQ SDK for PHP5"
    , "keywords": ["groovehq", "sdk"]
    , "type": "library"
    , "license": "MIT"
    , "authors": [
        {
            "name": "Jad Bitar"
            , "email": "jadbitar@mac.com"
            , "homepage": "http://jadb.io"
        }
    ]
    , "require": {
        "cakephp/utility": "^3.2"
        , "guzzlehttp/guzzle": "^6.0"
        , "guzzlehttp/guzzle-services": "^0.5"
        , "sabre/xml" : "^1.4"
    }
    , "require-dev": {
